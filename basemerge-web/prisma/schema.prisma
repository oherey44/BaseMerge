 generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Score {
  id        String   @id @default(cuid())
  wallet    String
  score     Int
  seasonId  String
  createdAt DateTime @default(now())
}

model ShareTicket {
  id         String   @id @default(cuid())
  wallet     String
  consumed   Boolean  @default(false)
  awardedAt  DateTime @default(now())
  consumedAt DateTime?
  source     String   @default("farcaster")
}

model ShareQuest {
  id              String   @id @default(cuid())
  wallet          String
  type            String   @default("farcaster")
  lastCompletedAt DateTime?

  @@unique([wallet, type])
}
