#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules/solidity-coverage/plugins/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules/solidity-coverage/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules/solidity-coverage/plugins/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules/solidity-coverage/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules/solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22._98d77a6dddc8cabcf251c8d22a63cf22/node_modules/solidity-coverage/plugins/bin.js" "$@"
fi
