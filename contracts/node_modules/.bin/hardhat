#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules/hardhat/internal/cli/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules/hardhat/internal/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules/hardhat/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules/hardhat/internal/cli/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules/hardhat/internal/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules/hardhat/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/hardhat@2.26.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@22.7.5_typescript@5.9.3__typ_88879709af4815e235f6cb901f5eb16c/node_modules:/home/amozer/Masaüstü/proje/BaseMerge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
fi
